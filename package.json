{
  "name": "nodejs-pro-hoidanit",
  "version": "1.0.0",
  "description": "nodejs pro hoidanit",
  "main": "dist/app.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "nodemon",
    "dev": "nodemon",
    "start:debug": "nodemon --inspect=9229 -e ts,tsx --exec node -r ts-node/register -r tsconfig-paths/register ./src/app.ts"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts",
    "ignore": [
      "node_modules"
    ],
    "exec": "ts-node -r tsconfig-paths/register ./src/app.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/quanganh261299/nodejs-pro-hoidanit.git"
  },
  "keywords": [
    "nodejs",
    "express",
    "mongodb"
  ],
  "author": {
    "name": "hoidanit",
    "url": "https://hoidanit.vn"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/quanganh261299/nodejs-pro-hoidanit/issues"
  },
  "homepage": "https://github.com/quanganh261299/nodejs-pro-hoidanit#readme",
  "dependencies": {
    "@prisma/client": "6.3.0",
    "@types/express": "^5.0.0",
    "@types/node": "^22.10.7",
    "dotenv": "16.4.7",
    "ejs": "3.1.10",
    "express": "5.0.1",
    "mysql2": "3.12.0",
    "prisma": "6.3.0",
    "typescript": "^5.7.3"
  },
  "devDependencies": {
    "@types/ejs": "3.1.5",
    "nodemon": "3.1.9",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0"
  }
}
